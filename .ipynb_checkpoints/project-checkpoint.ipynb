   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import random\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.linear_model import LinearRegression\n",
    "import shap\n",
    "import time\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "45db3d71",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_direction(playDescription):\n",
    "        if(\"left\" in playDescription):\n",
    "                return 0\n",
    "        if(\"right\" in playDescription):\n",
    "                return 2\n",
    "        return 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "1b02ce5d-b259-42ed-a76b-55ddc8790023",
   "metadata": {},
   "outputs": [],
   "source": [
    "# load dataframes\n",
    "games = pd.read_csv(\"data/games.csv\")\n",
    "plays = pd.read_csv(\"data/plays.csv\")\n",
    "tackles = pd.read_csv(\"data/tackles.csv\")\n",
    "players = pd.read_csv(\"data/players.csv\")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5de7b173",
