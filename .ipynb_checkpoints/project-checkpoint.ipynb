   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "3b23e19e-387a-417b-836b-e7fb3aff4bc7",
   "metadata": {},
   "outputs": [],
   "source": [
    "# add target var (can also use play desc to count scrambles as runs)\n",
    "plays[\"run\"] = plays[\"passResult\"].isna()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "208f39dc-c535-417c-9930-56802347bad0",
   "metadata": {},
   "outputs": [],
   "source": [
    "#makes column for if run(1 true, 0 false)\n",
    "plays[\"run\"] = plays[\"passResult\"].isna()\n",
    "#makes column for play direction\n",
    "plays[\"direction\"] = plays[\"playDescription\"].apply(get_direction)\n",
    "#filters play data\n",
    "plays_filtered = plays[[\"gameId\",\"quarter\", \"down\", \"yardsToGo\", \"possessionTeam\",\"defensiveTeam\",\"absoluteYardlineNumber\", \"gameClock\", \"preSnapHomeScore\",\n",
    "                        \"preSnapVisitorScore\", \"offenseFormation\", \"defendersInTheBox\",\"run\",\"direction\"]]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "a6681d7b-80c8-4bde-9409-d5fbc6acf911",
   "metadata": {},
   "outputs": [],
   "source": [
    "#filters game data\n",
    "games_filtered = games[[\"gameId\", \"week\",\"gameTimeEastern\"]]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "ad22c6e8-db6f-42ac-9386-ebb63ac2e8b8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# filter players dataframe\n",
    "players_filtered = players[[\"position\", \"nflId\"]]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "fb3a00b0-0027-492c-8cae-d8650d92b02e",
   "metadata": {},
   "outputs": [],
   "source": [
    "#combine game data with play data\n",
    "data = plays_filtered.merge(games_filtered, on=\"gameId\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "40e6e5df-b054-472f-8200-273d40622ac2",
   "metadata": {},
   "outputs": [],
   "source": [
    "#make all game times purly numbers (probably not necessary - most models can handle datetime or can convert to int differently)\n",
    "def str_to_time(time):\n",
    "    splt = time.split(\":\")\n",
    "    return int(splt[0]) * 60 + int(splt[1])\n",
    "data[\"gameTimeEastern\"] = data[\"gameTimeEastern\"].apply(str_to_time)\n",
    "data[\"gameClock\"] = data[\"gameClock\"].apply(str_to_time)\n",
    "#drop game id\n",
    "data.drop(\"gameId\",axis =1, inplace=True)\n",
    "#normalization]\n",
    "for param in ['yardsToGo','absoluteYardlineNumber',\t'gameClock', 'week','preSnapHomeScore',\t'preSnapVisitorScore',\t'defendersInTheBox','gameTimeEastern']:\n",
    "    data[param] = (data[param] - data[param].mean())/data[param].std()\n",
    "    \n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "7375688e",
   "metadata": {},
   "outputs": [],
   "source": [
    "#find all unique teams\n",
    "teams = data.possessionTeam.unique()\n",
    "#1 hot encode teams and formations\n",
    "data = pd.get_dummies(data, prefix=['possessionTeam', 'defensiveTeam', 'offenseFormation'])\n",
    "#fill 1 nan in defeive players in the box\n",
    "data.fillna(0,inplace=True)"
   ]
